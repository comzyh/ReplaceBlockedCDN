{
	"name" : "ReplaceBlockedCDN",
	"version" : "0.0.1",
	"manifest_version" : 2,
	"icons" : {
		"48" : "Icon.png"
	},
	"description" : "替换掉被屏蔽的CDN,如Google API",
	"content_scripts" : [{
			"js" : ["inject.js"],
			"matches" : ["http://*/*"],
			"run_at" : "document_start"
		}
	],
	"options_page" : "options.html",
	"permissions" : ["http://*/*"]
}
